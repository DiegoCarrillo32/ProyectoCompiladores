<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewModels="clr-namespace:AvaloniaApplication5.ViewModels"
        xmlns:views="clr-namespace:AvaloniaApplication5.Views"
        x:Class="AvaloniaApplication5.Views.ConsoleWindow"
        x:DataType="viewModels:ConsoleWindowViewModel"   
        Title="Console"
        Width="500" Height="500">
    <Design.DataContext>
        <viewModels:ConsoleWindowViewModel/>
    </Design.DataContext>
    
    <Grid>
        <TextBlock Text="{Binding Text}"  Name="text_block"> </TextBlock>
        
    </Grid>
</Window>